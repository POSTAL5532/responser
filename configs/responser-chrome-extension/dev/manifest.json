{
  "manifest_version": 3,
  "name": "Reviewly DEV",
  "version": "1.0",
  "description": "Chrome extensions which helps to leve the rating and reviews about the web site experience.",
  "key": "",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "scripting"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Reviewly DEV",
    "default_icon": {
      "16": "logo_16.png",
      "32": "logo_32.png",
      "64": "logo_64.png",
      "128": "logo_128.png"
    }
  },
  "background": {
    "service_worker": "./background.js"
  },
  "externally_connectable": {
    "matches": [
      "http://dev-app.reviewly.space/*"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": ["content-styles.css"],
      "js": [
        "./content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "fonts/*"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "icons": {
    "16": "logo_16.png",
    "32": "logo_32.png",
    "64": "logo_64.png",
    "128": "logo_128.png"
  }
}