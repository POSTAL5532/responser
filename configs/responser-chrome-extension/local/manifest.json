{
  "manifest_version": 3,
  "name": "Reviewly LOCAL DEV",
  "version": "1.0",
  "description": "Chrome extensions which helps to leve the rating and reviews about the web site experience.",
  "key": "",
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Reviewly LOCAL DEV",
    "default_icon": {
      "16": "logo_16.png",
      "32": "logo_32.png",
      "64": "logo_64.png",
      "128": "logo_128.png"
    }
  },
  "background": {
    "service_worker": "./background.js"
  },
  "externally_connectable": {
    "matches": [
      "http://localhost:3000/*",
      "http://127.0.0.1:3000/*"
    ]
  },
  "icons": {
    "16": "logo_16.png",
    "32": "logo_32.png",
    "64": "logo_64.png",
    "128": "logo_128.png"
  }
}