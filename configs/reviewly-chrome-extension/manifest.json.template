{
  "manifest_version": 3,
  "name": "Reviewly LOCAL",
  "version": "1.0",
  "description": "Chrome extensions that assist in exploring and leaving ratings and reviews about website experiences.",
  "key": "",
  "homepage_url": "http://local-dev-reviewly.space",
  "permissions": [
    "tabs",
    "storage",
    "scripting"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Click for see or leave opinions about this web resource.",
    "default_icon": {
      "16": "logo_16.png",
      "32": "logo_32.png",
      "64": "logo_64.png",
      "128": "logo_128.png"
    }
  },
  "background": {
    "service_worker": "./background.js"
  },
  "externally_connectable": {
    "matches": [
      "http://local-dev-reviewly.space/*"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "content-styles.css"
      ],
      "js": [
        "./content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "fonts/*"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "icons": {
    "16": "logo_16.png",
    "32": "logo_32.png",
    "64": "logo_64.png",
    "128": "logo_128.png"
  }
}